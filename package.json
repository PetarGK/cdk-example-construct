{
  "name": "cdk-example-construct",
  "version": "1.0.0",
  "description": "An AWS CDK Construct that enforces encryption on an S3 bucket",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "test": "tsc && jest",
    "watch": "tsc -w"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "pk.secure-bucket",
        "module": "pk.secure_bucket"
      },      
      "java": {
        "package": "com.pk.securebucket",
        "maven": {
          "groupId": "com.pk",
          "artifactId": "secure-bucket"
        }
      },
      "dotnet": {
        "namespace": "PK.SecureBucket",
        "packageId": "PK.SecureBucket"
      }
    }
  },
  "repository": {
    "url": "https://github.com/PetarGK/cdk-example-construct.git",
    "type": "git"
  },
  "author": {
    "name": "Petar Korudzhiev",
    "email": "petar.korudzhiev@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "@aws-cdk/assert": "^1.27.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "jest": "^25.1.0",
    "jsii": "^1.0.0",
    "jsii-pacmak": "^1.0.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@aws-cdk/aws-s3": "^1.27.0",
    "@aws-cdk/core": "^1.27.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-s3": "^1.27.0",
    "@aws-cdk/core": "^1.27.0"
  }
}
